        if ($(".progress .progress-bar")[0]) {
            $('.progress .progress-bar').progressbar(); // bootstrap 3
        }
    </script>*@

    <div class="modal de fade" id="myModal1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" id="modelContent1">
            </div>
        </div>
    </div>
    <script>
        function createModal(url) {
            $('#modelContent1').load(url);
            $('.de').modal('show');
        }
    </script>
    <script src='~/ags-mobile/build/js/jQuery.Keyframes-master/dist/jquery.keyframes.min.js'></script>
    <script>

        $(document).ready(
    function () {
        var test1 = $('div.p1').text();
        var test2 = $('div.p2').text();
        var test3 = $('div.p3').text();
        var test4 = $('div.p4').text();
        var arlene2 = new Array(test1, test2, test3, test4);
        $.each(arlene2, function (index, value) {
            //alert(value);
         
            var v1 = parseFloat(value);
                
    
            if (test1 == value) {
                var dv180 = 0;
                var dv1 = 0;

                dv180 = (v1 * 360) / 100;
                if (dv180 >= 180) {
                    dv1 = dv180 - 180;
                }
                else {
                    dv1 = dv180;
                }



                $.keyframe.define([{
                    name: 'loading',
                    '0%': { '-webkit-transform': 'rotate(0deg)', 'transform': 'rotate(0deg)' },
                    '100%': { '-webkit-transform': 'rotate(' + dv1 + 'deg)', 'transform': 'rotate(' + dv1 + 'deg)' }
                }]);
            }
            else if (test2 == value) {
                var dv280 = 0;
                var dv2 = 0;

                dv280 = (v1 * 360) / 100;
                if (dv280 >= 180) {
                    dv2 = dv280 - 180;
                }
                else {
                    dv2 = dv280;
                }



                $.keyframe.define([{
                    name: 'loading12',
                    '0%': { '-webkit-transform': 'rotate(0deg)', 'transform': 'rotate(0deg)' },
                    '100%': { '-webkit-transform': 'rotate(' + dv2 + 'deg)', 'transform': 'rotate(' + dv2 + 'deg)' }
                }]);
            }
            else if (test3 == value) {
                var dv380 = 0;
                var dv3 = 0;

                dv380 = (v1 * 360) / 100;
                if (dv380 >= 180) {
                    dv3 = dv380 - 180;
                }
                else {
                    dv3 = dv380;
                }



                $.keyframe.define([{
                    name: 'loading13',
                    '0%': { '-webkit-transform': 'rotate(0deg)', 'transform': 'rotate(0deg)' },
                    '100%': { '-webkit-transform': 'rotate(' + dv3 + 'deg)', 'transform': 'rotate(' + dv3 + 'deg)' }
                }]);
            }
            else if (test4 == value) {
                var dv480 = 0;
                var dv4 = 0;

                dv480 = (v1 * 360) / 100;
                if (dv480 >= 180) {
                    dv4 = dv480 - 180;
                }
                else {
                    dv4 = dv480;
                }



                $.keyframe.define([{
                    name: 'loading14',
                    '0%': { '-webkit-transform': 'rotate(0deg)', 'transform': 'rotate(0deg)' },
                    '100%': { '-webkit-transform': 'rotate(' + dv4 + 'deg)', 'transform': 'rotate(' + dv4 + 'deg)' }
                }]);
            }

            if (v1 >= 50)
            {
                if (test1 == value)
                {
                    $("span.pright1").children().css({ "animation": "loading-1 0.2s linear forwards 1.8s" });

                    $("span.pleft1").children().css({ "animation": "loading 3.7s linear forwards 1.8s" });

                }
                else if (test2 == value) {
                    $("span.pright2").children().css({ "animation": "loading-1 0.2s linear forwards 1.8s" });

                    $("span.pleft2").children().css({ "animation": "loading12 3.7s linear forwards 1.8s" });

                }
                else if (test3 == value) {
                    $("span.pright3").children().css({ "animation": "loading-1 0.2s linear forwards 1.8s" });

                    $("span.pleft3").children().css({ "animation": "loading13 3.7s linear forwards 1.8s" });

                }
                else if (test4 == value) {
                    $("span.pright4").children().css({ "animation": "loading-1 0.3s linear forwards 1.8s" });

                    $("span.pleft4").children().css({ "animation": "loading14 3.7s linear forwards 1.8s" });

                }
               
            }
            else 
            {
                if (test1 == value) {
                $("span.pright1").children().css({ "animation": "loading 1.2s linear forwards 1.8s" });
                $("span.pleft1").children().css({ "animation": "loading-5  linear forwards 1.8s" });
              }
                else if (test2 == value) {
                  $("span.pright2").children().css({ "animation": "loading12 1.2s linear forwards 1.8s" });
                  $("span.pleft2").children().css({ "animation": "loading-5  linear forwards 1.8s" });
              }
                else if (test3 == value) {
                  $("span.pright3").children().css({ "animation": "loading13 1.2s linear forwards 1.8s" });
                  $("span.pleft3").children().css({ "animation": "loading-5  linear forwards 1.8s" });
              }
                else if (test4 == value) {
                  $("span.pright4").children().css({ "animation": "loading14 1.2s linear forwards 1.8s" });
                  $("span.pleft4").children().css({ "animation": "loading-5  linear forwards 1.8s" });
              }
            }

        });
    }
);






    </script>

    <div class="modal fade" id="myModal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" id="modelContent">
                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">Logout</h4>
                    <button type="button" class="close" data-dismiss="modal" style="margin-top: -24px;">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    Are you sure want to Logout
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <a href="@Url.Content("/Account/MobileLogout/")" class="btn btn-success" role="button">Yes</a>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">No</button>
                </div>
            </div>
        </div>
    </div>

</body>
</html>
